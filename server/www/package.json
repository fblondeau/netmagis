  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdav/netmagis.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdir -p dist && webpack -d --config webpack.config.js --bail --color=false --hide-modules --devtool source-map",
    "buildprod": "mkdir -p dist && webpack -p --config webpack.config.js --bail --color=false",
    "machin": "babel scripts/translate.js | node"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sprintf-js": "^1.1.1",
    "universal-cookie": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "jscodeshift": "^0.5.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-react-jsx-source",
      "react-intl"
    ]
  }
}
